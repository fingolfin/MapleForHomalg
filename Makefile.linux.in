
bin/@GAPARCH@/mapleforhomalg.o: src/mapleforhomalg.c
	cp $(GAPPATH)/bin/@GAPARCH@/config.h bin/@GAPARCH@/
	$(CC) -fPIC -Wall -O2 -pipe -I"$(MAPLEINCLUDEPATH)" -o bin/@GAPARCH@/mapleforhomalg.o -I$(GAPPATH)/bin/@GAPARCH@/../.. -I$(GAPPATH)/bin/@GAPARCH@ -DCONFIG_H -c src/mapleforhomalg.c

bin/@GAPARCH@/mapleforhomalg.so: bin/@GAPARCH@/mapleforhomalg.o
	$(CC) -L "$(MAPLEBINPATH)" -fPIC -shared -lmaplec -o bin/@GAPARCH@/mapleforhomalg.so bin/@GAPARCH@/mapleforhomalg.o
